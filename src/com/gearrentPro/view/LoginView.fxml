<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ancLogin" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gearrentPro.controller.LoginController">
    <children>
        <Label layoutX="217.0" layoutY="32.0" text="Login">
            <font>
                <Font size="35.0" />
            </font>
        </Label>
        <Label layoutX="118.0" layoutY="121.0" text="User Nane: " />
        <Label layoutX="118.0" layoutY="176.0" text="Password: " />
        <PasswordField fx:id="txtPassword" layoutX="205.0" layoutY="172.0" promptText="Password" />
        <TextField fx:id="txtUsername" layoutX="205.0" layoutY="117.0" promptText="Username" />
        <Button fx:id="btnLogin" layoutX="245.0" layoutY="236.0" mnemonicParsing="false" onAction="#loginOnAction" text="LOGIN" />
    </children>
</AnchorPane>
